<!DOCTYPE html>
<html>
<head>
<link rel="stylesheet" href="css/AdminLTE.min.css">
<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">	
	<title></title>
	 
        <link rel="stylesheet" href="AdminLTE.min.css">
</head>
<body class="jumbotron">
	
		<form method="post">
			<div class="col-md-3">
		
	</div>
	<div class="col-md-6">
		<div class="box box-primary">
			<div class="box-header with-border">
	              <h3 class="box-title" style="margin-left: 35%;">Registration</h3>
	            </div>
	            <div class="box-body">
	            	<form method="post">
	            	<div class="form-group">
							<div class="row">
							<div class="col-md-3">
								<label>User Name </label>
							</div>
							<div class="col-md-6">
								<input type="text" name="uname" class="form-control">  
							</div>
						</div>
						</div>
	            	 
					  <div class="form-group">
							<div class="row">
							<div class="col-md-3">
								<label>Email  </label>
							</div>
							<div class="col-md-6">
								<input type="email" name="email" class="form-control">  
							</div>
						</div>
						</div>
				  
				  		<div class="form-group">
							<div class="row">
							<div class="col-md-3">
								<label> Mobile   </label>
							</div>
							<div class="col-md-6">
								<input type="text" name="mobile" class="form-control">  
							</div>
						</div>
						</div>
					    
					  	<div class="form-group">
							<div class="row">
							<div class="col-md-3">
								<label>  District   </label>
							</div>
							<div class="col-md-6">
								<select name="dis" class="form-control">
							<% for(var i = 0; i < dis.length; i++) { %>
		
							<option value="<%= dis[i].District_ID %>">
							
							<%= dis[i].District_Name %>
								
							</option>
			
		
							<% } %>
						</select>
							</div>
						</div>
						</div>
						
						<div class="form-group">
							<div class="row">
							<div class="col-md-3">
								<label>Password  </label>
							</div>
							<div class="col-md-6">
								<input type="password" name="pass" class="form-control"> 
							</div>
						</div>
						</div>
						 <div class="form-group">
							<div class="row">
							<div class="col-md-3">
								<label> Retype Password </label>
							</div>
							<div class="col-md-6">
								<input type="password" name="rpass" class="form-control">  
							</div>
						</div>
						</div>
						<div class="form-group">
							<div class="row">
							<div class="col-md-3">
							
							</div>
							<div class="col-md-6">
								<input type="submit" class="btn btn-primary">  
							</div>
						</div>
						</div>
						 
					</form>	
	            	<ul>
	<% data.errors.forEach(function(err){ %>
		<li style="color: red;"><%= err.msg %></li>
	<% }) %>
	</ul>
	<li style="color: red;">
	<%=perror%>
	</li>
				</div>
				<div class="box-footer " >
						<div class="row">
							<div class="col-md-3">
								<label>Already Registered</label>
							</div>
							<a href="/login" class="btn btn-info">Signin now</a>
						</div>
				
					</div>
					<footer class="main-footer">
    <div class=" hidden-xs">
      <b>Version</b> 1.0.0
    </div>
    <strong>Copyright Node JS project Team</strong> All rights
    reserved.
  </footer>
	            </div>
				  
					
	
</body>
</html>